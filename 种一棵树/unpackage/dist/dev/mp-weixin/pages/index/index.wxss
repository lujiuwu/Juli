}
.index .swiper {
  height: 370rpx;
}
.index .image-swiper swiper-item image {
  width: 100%;
  height: 530rpx;
}
.index .garden-ranking-list {
  width: 100%;
  height: 580rpx;
  margin-top: 20rpx;
}
.index .garden-ranking-list .head {
  padding: 10rpx 24rpx;
  display: flex;
}
.index .garden-ranking-list .head .title {
  font-size: 36rpx;
  flex: 5;
}
.index .garden-ranking-list .head .subsetion-choice {
  flex: 4;
}
.index .garden-ranking-list .body {
  padding: 10rpx 24rpx;
  height: 450rpx;
}
.index .garden-ranking-list .body .list-body {
  border-radius: 20rpx;
  height: 100%;
  background: #F2DA98;
  padding: 20rpx;
}
.index .garden-ranking-list .body .list-body .myInfo {
  height: 94rpx;
  border-radius: 10px 10px 0 0;
  display: flex;
  line-height: 94rpx;
  background-color: #f3cf84;
  border-bottom: 4px solid #f3bd1a;
}
.index .garden-ranking-list .body .list-body .myInfo .my-rank {
  flex: 1;
  text-align: center;
  color: #FB5608;
  font-size: 60rpx;
  font-weight: bold;
}
.index .garden-ranking-list .body .list-body .myInfo .my-avatar {
  flex: 1;
  padding: 10rpx;
  border-radius: 100px;
  overflow: hidden;
  margin-right: 10rpx;
}
.index .garden-ranking-list .body .list-body .myInfo .my-avatar .avatar-image {
  width: 100%;
  height: 100%;
}
.index .garden-ranking-list .body .list-body .myInfo .my-username {
  flex: 5;
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
}
.index .garden-ranking-list .body .list-body .myInfo .my-username .des {
  color: #737373;
  font-weight: normal;
  margin-left: 10rpx;
}
.index .garden-ranking-list .body .list-body .myInfo .rank-number {
  flex: 1;
  font-size: 34rpx;
  color: #00973B;
  font-weight: bold;
}
.index .garden-ranking-list .body .list-body .all-rank {
  margin-top: 10rpx;
  height: 94rpx;
  display: flex;
  line-height: 94rpx;
  background-color: #F2EEBD;
}
.index .garden-ranking-list .body .list-body .all-rank .single-rank {
  flex: 1;
  height: 35px;
  width: 10px;
}
.index .garden-ranking-list .body .list-body .all-rank .single-rank .rank-image {
  width: 100%;
  height: 100%;
}
.index .garden-ranking-list .body .list-body .all-rank .single-avatar {
  flex: 1;
  padding: 10rpx;
  overflow: hidden;
  border-radius: 100px;
  margin-right: 10rpx;
}
.index .garden-ranking-list .body .list-body .all-rank .single-avatar .avatar-image {
  width: 100%;
  height: 100%;
}
.index .garden-ranking-list .body .list-body .all-rank .single-username {
  flex: 5;
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
}
.index .garden-ranking-list .body .list-body .all-rank .single-rank-number {
  flex: 1;
  font-size: 32rpx;
  color: #00973B;
  font-weight: bold;
}
.index .garden-ranking-list .body .list-body .showMore {
  display: flex;
  margin-top: 14rpx;
  text-align: right;
  color: #323232;
  font-size: 34rpx;
}
.index .garden-ranking-list .body .list-body .showMore .text {
  flex: 19;
  margin-right: 10rpx;
}
.index .garden-ranking-list .body .list-body .showMore .icon {
  flex: 1;
}
.index .getPoints {
  padding: 10rpx 24rpx;
}
.index .getPoints .head {
  font-size: 36rpx;
}
.index .getPoints .body {
  margin-top: 20rpx;
  display: flex;
  gap: 14rpx;
}
.index .getPoints .body .get-choice {
  flex: 1;
  height: 240rpx;
  border-radius: 28rpx 28rpx 100rpx 28rpx;
  position: relative;
  text-align: center;
}
.index .getPoints .body .get-choice .text {
  width: 100rpx;
  font-size: 42rpx;
  position: absolute;
  top: 100rpx;
  left: 12rpx;
  color: #fff;
  text-align-last: left;
}
.index .getPoints .body .get-choice .get-add {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
  background-color: #fff;
  position: absolute;
  bottom: 0;
  right: 0;
  line-height: 100rpx;
  text-align: center;
  font-weight: bold;
  font-size: 92rpx;
  color: #FCA73F;
  border: 8rpx solid #FCA73F;
}
.index .popup-box {
  height: 200rpx;
  padding: 50rpx 30rpx;
}
.index .popup-box .test {
  text-align: center;
  font-size: 40rpx;
}
.index .popup-box .upload {
  margin-top: 30rpx;
}

